#Options +Includes +FollowSymLinks -IndexesOptions +IncludesOptions +FollowSymLinksOptions -IndexesAddHandler server-parsed .shtmlDirectoryIndex index.shtml index.htm index.php index.htmlAddDefaultCharset windows-1251CookieExpires "1 years"php_value session.gc_maxlifetime 1080000php_value session.cookie_lifetime 1080000# ������� � ��, ��� � ���� �������php_flag magic_quotes_gpc Offphp_flag magic_quotes_runtime Offphp_flag magic_quotes_sybase Off# ������php_flag session.use_trans_sid Offphp_flag session.use_only_cookies Offphp_flag session.use_cookies On# ������php_flag register_globals Offphp_flag short_open_tag Onphp_value variables_order "EGPCS"php_value gpc_order "GPC"php_value default_mimetype "text/html"# �������� (������)php_value post_max_size 20Mphp_value upload_max_filesize 20Mphp_value max_file_uploads 100# ������php_value log_errors Onphp_value log_errors_max_len 5242880php_value ignore_repeated_errors Onphp_flag track_errors Onphp_flag display_startup_errors OnRewriteEngine onOptions +FollowSymlinksRewriteBase /RewriteCond %{REQUEST_FILENAME} -dRewriteRule .* - [L]RewriteCond %{REQUEST_FILENAME} -fRewriteRule .* - [L]RewriteCond %{REQUEST_FILENAME} !.(css)|(jpg)|(gif)|(png)|(js)$RewriteRule ^([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1 [L,QSA]RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1&menu2=$2 [L,QSA]RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1&menu2=$2&menu3=$3 [L,QSA]RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1&menu2=$2&menu3=$3&menu4=$4 [L,QSA]RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1&menu2=$2&menu3=$3&menu4=$4&menu5=$5 [L,QSA]RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1&menu2=$2&menu3=$3&menu4=$4&menu5=$5&menu6=$6 [L,QSA]RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)[/]?$ /index.php?menu=$1&menu2=$2&menu3=$3&menu4=$4&menu5=$5&menu6=$6&menu7=$7 [L,QSA]